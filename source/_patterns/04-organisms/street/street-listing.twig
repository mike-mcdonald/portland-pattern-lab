{% set classification_map = {
  '': '',
 } %}
<article>
  <h1>{{ name }}</h1>
  <p class="text-2xl text-gray-700">{{ block }}
    block</p>
  <div class="flex flex-wrap items-center mb-3">
    <dl>
      <div class="flex flex-wrap items-center">
        <dt>Transportation planning ID:</dt>
        <dd class="ml-4">{{ id }}</dd>
      </div>
    </dl>
  </div>
  <h2>Classifications</h2>
  <dl>
    <div class="flex flex-wrap items-center mb-2">
      <dt>Bicycle clasification:</dt>
      <dd class="ml-4">
        <a href="#">Major City Bikeway</a>
      </dd>
    </div>
    <div class="flex flex-wrap items-center mb-2">
      <dt>Transit classification:</dt>
      <dd class="ml-4">
        <a href="#">Regional Transitway</a>
      </dd>
    </div>
    <div class="flex flex-wrap items-center mb-2">
      <dt>Street design classification:</dt>
      <dd class="ml-4">
        <a href="#">Urban Throughway</a>
      </dd>
    </div>
    <div class="flex flex-wrap items-center mb-2">
      <dt>Traffic clasification:</dt>
      <dd class="ml-4">
        <a href="#">Regional Transitway</a>
      </dd>
    </div>
    <div class="flex flex-wrap items-center mb-2">
      <dt>Freight clasification:</dt>
      <dd class="ml-4">
        <a href="#">Regional Transitway</a>
      </dd>
    </div>
    <div class="flex flex-wrap items-center mb-2">
      <dt>Emergency clasification:</dt>
      <dd class="ml-4">
        <a href="#">Regional Transitway</a>
      </dd>
    </div>
    <div class="flex flex-wrap items-center mb-2">
      <dt>Pedestrian classification:</dt>
      <dd class="ml-4">
        <a href="#">Off-street Path</a>
      </dd>
    </div>
  </dl>
</details>
<h2>Projects affecting this street</h2>
<ul class="m-0">
  {% for project in projects %}
    <li class="my-2">
      <a href="#">{{ project.name }}</a>
    </li>
  {% endfor %}
</ul></article>
